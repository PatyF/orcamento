<!DOCTYPE html>
<html>
<head>
  <title>Gestão Financeira</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<nav class="navbar navbar-inverse">
  <div class="container">
    <div class="navbar-header">
      <a class="navbar-brand" href="/budgets">Gestão Financeira</a>
    </div>
    <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
      <ul class="nav navbar-nav">
        <li><a href="/accounts">Account</a></li>
        <li class="active"><a href="/budgets">Budget</a></li>
        <li><a href="/categories">Category</a></li>
        <li><a href="/movements">Movement</a></li>
        <li><a href="/reports">Report</a></li>
      </ul>
      <ul class="nav navbar-nav navbar-right">
         <li>
           <%= link_to "Log out", sign_out_path, method: "delete" %>
         </li>
      </ul>
    </div>
  </div>
</nav>
<%= yield %>

</body>
</html>
